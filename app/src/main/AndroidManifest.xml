<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.jagtapazad.ivymrkevt">

    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:fullBackupContent="@xml/backup_descriptor"
        android:icon="@drawable/logoblue"
        android:label="@string/app_name"
        android:roundIcon="@drawable/logoblue"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:ignore="ExtraText">
        <activity
            android:name=".User.RegisterUser2"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Common.Dashboard"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Home"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Common.LeaderBoard"
            android:screenOrientation="portrait" />

        <activity
            android:name=".Game.GameMafia"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Game.GamePsych"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Game.GamePoker"
            android:screenOrientation="portrait" />

        <activity
            android:name=".Events.TreasureHunt"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Events.TreasureHunt2"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Common.AgreementPay"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Common.ComingSoon"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Common.HostEvent"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Common.EventNow"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Common.AboutUs"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Common.Notice"
            android:screenOrientation="portrait" />

        <activity
            android:name=".User.RegisterUser"
            android:screenOrientation="portrait" />
        <activity
            android:name=".User.Payment"
            android:screenOrientation="portrait" />
        <activity
            android:name=".User.Profile"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.journeyapps.barcodescanner.CaptureActivity"
            android:screenOrientation="portrait"
            tools:replace="screenOrientation" />
        <activity android:name=".User.LoginActivity" />
        <activity
            android:name=".Common.SplashScreen"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <service
            android:name=".HelperClasses.MyMessagingService"
            android:enabled="true"
            android:exported="false">
            <intent-filter>
                <action android:name="com.google.firebase.MESSAGING_EVENT" />
            </intent-filter>
        </service>

        <meta-data
            android:name="com.google.firebase.messaging.default_notification_icon"
            android:resource="@drawable/logoblue" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>